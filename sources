
TARGETNAME=MsiDump
TARGETPATH=obj
TARGETTYPE=PROGRAM

UMTYPE=console
UMENTRY=wmain

TARGETLIBS= \
            $(SDK_LIB_PATH)\setupapi.lib	\
            $(SDK_LIB_PATH)\msi.lib

USE_WTL = 1
WTL_VER = 71
USE_STL = 1

USE_MSVCRT = 1
USE_NATIVE_EH = 1
USER_C_FLAGS  = -EHsc

C_DEFINES = $(C_DEFINES) -DUNICODE -D_UNICODE 

# comment out the following line to disable tracing
C_DEFINES = $(C_DEFINES) -DENABLE_TRACE=1

RCOPTIONS = -DPROJECT_VERSION=$(PROJECT_VERSION) -DPROJ=$(PROJ)

SOURCES= \
	MsiUtils.cpp	\
	MsiTable.cpp	\
	MsiDump.cpp	\
	ui.rc
